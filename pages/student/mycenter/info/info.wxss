

page{
  width: 100%;
  height: 100%;
  background-color: #f3f9fc;
  overflow: hidden;
}

.content{
  width: 100%;
  height: 178rpx;
  background-color: #fff;
  overflow: hidden;
  margin-top:20rpx;
}

.head{
  width: 120rpx;
  height: 120rpx;
  border-radius: 100%;
  float: right;
  
}


.list{
  width: 50%;
  background-color: #fff;
  height: 100%;
  float: left;
}

.number{
  line-height: 178rpx;
  overflow: hidden;
  font-size:32rpx;
  padding-left:34rpx;
}

.floatr{
  padding-right:32rpx;
  padding-top:30rpx;
  position: relative;
}
.handelicon{
  width: 30rpx;
  height: 20rpx;
  transform:rotate(270deg);
  -ms-transform:rotate(270deg); /* Internet Explorer */
  -moz-transform:rotate(270deg); /* Firefox */
  -webkit-transform:rotate(270deg); /* Safari å’Œ Chrome */
  -o-transform:rotate(270deg); /* Opera */
  margin:32rpx 32rpx 32rpx 0rpx;
}

.camera{
  width: 32rpx;
  height: 26rpx;
  position: absolute;
  left: 270rpx;
  top:100rpx
}

.fade{
  width: 120rpx;
  height: 60rpx;
  background-color: #000;
  opacity: 0.6;
  position: absolute;
  left: 225rpx;
  top:90rpx;
  border-radius:0 0 50px 50px; 
}

.grayLine{
  width: 85%;
  margin-left:15%;
  height: 2rpx;
  background-color: #eee;
}

.item{
  height: 100rpx;
  width: 100%;
  background-color: #fff;
  overflow: hidden;
}

.leftlist{
  width: 45%;
  line-height: 100rpx;
  padding-left:30rpx;
  font-size: 32rpx;
  color:#666;
}

.rightlist{
  width: 45%;
  float: right;
  text-align: right;
  padding-right:30rpx;
  line-height: 100rpx;
  font-size: 32rpx;
  color:#999;
}

.quit{
  width: 688rpx;
  height: 80rpx;
  display: block;
  margin:60rpx auto 0 auto;
}